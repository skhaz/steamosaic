{
  "hosting":{
    "public":"public",
    "ignore":[
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites":[
      {
        "source":"**",
        "destination":"/index.html"
      }
    ],
    "headers":[
      {
        "source":"/",
        "headers":[
          {
            "key":"Link",
            "value":"</js/app.js>;rel=preload;as=script,</css/styles.css>;rel=preload;as=style"
          }
        ]
      }
    ]
  }
}